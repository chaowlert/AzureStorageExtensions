<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>2.3.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>chaowlert</Authors>
    <PackageProjectUrl>https://github.com/chaowlert/AzureStorageExtension</PackageProjectUrl>
    <PackageTags>scl azure storage</PackageTags>
    <Product>Azure Storage Extension</Product>
    <Description>Extensions for Azure Storage Client library</Description>
    <Copyright>Copyright (c) 2014 Chaowlert Chaisrichalermpol</Copyright>
    <RepositoryUrl>https://github.com/chaowlert/AzureStorageExtension</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Cosmos.Table" Version="1.0.7" />
    <PackageReference Include="Microsoft.Azure.Storage.Blob" Version="11.1.4" />
    <PackageReference Include="Microsoft.Azure.Storage.Queue" Version="11.1.4" />
  </ItemGroup>

</Project>
